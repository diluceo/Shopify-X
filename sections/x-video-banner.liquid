<div id="shopx-video-banner-{{ section.id }}">
  <h1>{{ section.settings.heading }}</h1>
</div>

{% schema %}
{
  "name": "ShopX - Video Banner",
  "tag": "section",
  "class": "section-wrapper",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Lorem Ipsum"
    }
  ],
  "presets": [
    {
      "name":"ShopX - Video Banner"
    }
  ]
}
{% endschema %}